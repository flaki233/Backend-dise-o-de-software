{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "RECAPTCHA_SECRET_KEY": "",
    "RECAPTCHA_SITE_KEY": "",
    "RECAPTCHA_TEST": "true",
    "JWT_SECRET": "",
    "ROBLE_API_URL": "https://roble-api.openlab.uninorte.edu.co",
    "ROBLE_TOKEN": ""
  }
}
